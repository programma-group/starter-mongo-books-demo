{
  "exclude": [
    "coverage/**",
    "test/**",
    "src/start.js",
    "testServer.js",
    "src/data/**",
    "src/utils/errorHandlers.js"
  ],
  "check-coverage": true,
  "per-file": true,
  "lines": 85,
  "functions": 99,
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "cache": true,
  "all": true,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
